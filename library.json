{
  "name": "pins",
  "description": "hardware IO abstraction",
  "repository":
  {
    "type": "git",
    "url": "https://github.com/iocafe/pins.git"
  },
  "frameworks": [
    "arduino", 
    "espidf"
 ],
 "platforms": [
   "teensy", 
   "stm32",
   "espressif32"
 ],
  
  "build": {
    "srcFilter": [
      "+<code/*>",
      "+<extensions/*>"
    ],
    "libLDFMode" : "chain"
  },
  
  "dependencies": [
    {
      "name": "iocom"
    }  
  ]
}
